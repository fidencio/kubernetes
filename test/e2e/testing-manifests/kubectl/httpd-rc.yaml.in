apiVersion: v1
kind: ReplicationController
metadata:
  name: httpd-rc
spec:
  replicas: 1
  selector:
    run: httpd-rc
  template:
    metadata:
      labels:
        run: httpd-rc
    spec:
      runtimeClassName: kata-qemu
      containers:
      - image: {{.HttpdNewImage}}
        name: httpd-rc
